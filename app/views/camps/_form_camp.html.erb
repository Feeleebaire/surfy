<%= simple_form_for [@organisation, Camp.new] do |f| %>
  <%= f.input :title, placeholder: "Amazing surf class", label: false, :class => "form-field" %>
    <%= f.input :description, placeholder: "Amazing surf class", label: false, :class => "form-field" %>
      <%= f.input :address, placeholder: "76 California st, 03114 Malibu", label: false, :class => "form-field" %>
    <%= f.input :language, collection: ["English", "French","German", "Spanish" ] %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>


<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyBHiojnI6bIOo1eIwu0CxV6h-fDg9bgnh4"></script>
<% end %>
